import random
lowercase = "abcdefghijklmnopqrstuvwxyz"
uppercase = "ABCDEFGHIJKLMNOPQRSTUVWSYZ"
number = "1234567890"
symbol = "!@#$%^&*"
all_character = lowercase + uppercase + number + symbol
digit = int(input("Input the length of your password"))
password = " "
for i in range(digit):
    password += (random.choice(all_character))
print(password)
